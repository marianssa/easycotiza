<p id="notice"><%= notice %></p>

<h1>Calificaciones</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Nota</th>
      <th>empresa id</th>
      <th colspan="3"></th>

    </tr>
  </thead>

  <tbody>
    <% @calificacions.each do |calificacion| %>
      <tr>
        <td><%=calificacion.id %></td>
        <td><%= calificacion.nota %></td>
        <td><%=calificacion.enterprise_id%></td>
        <td><%= link_to 'Show', client_calificacion_path(id:current_client.id) %></td>
        <td><%= link_to 'Edit', edit_client_calificacion_path(id:current_client.id) %></td>
        <td><%= link_to 'Destroy',client_calificacion_path(id:current_client.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calificacion', new_client_calificacion_path(current_client.id) %>
