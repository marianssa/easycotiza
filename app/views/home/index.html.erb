      <script src="jquery.ui.datepicker-es.js"></script>

      <script>
      $(function () {
      $.datepicker3.setDefaults($.datepicker3.regional["es"]);
      $("#datepicker").datepicker3({
      firstDay: 1,
      onSelect: function (date) {
      alert(date)
      },
      });
      });
      </script>


