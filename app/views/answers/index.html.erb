<p id="notice"><%= notice %></p>

<h1>Answers</h1>

<table>
  <thead>
    <tr>
      <th>Answer cotiza</th>
      <th>Accept date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answers.each do |answer| %>
      <tr>
        <td><%= answer.answer_cotiza %></td>
        <td><%= answer.accept_date %></td>
        <td><%=answer.cotizacion_id%></td>
        <td><%= link_to 'Show', enterprise_answers_path %></td>
        <td><%= link_to 'Edit', edit_enterprise_answer_path(@enterprise,answer) %></td>
        <td><%= link_to 'Destroy',enterprise_answer_path(@enterprise,answer),method: :delete, data: { confirm: 'Esta seguro?' }  %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

